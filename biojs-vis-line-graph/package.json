  },
  "sniper": {
    "js": [
      "/build/test.js",
      "https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.min.js",
      "https://cdnjs.cloudflare.com/ajax/libs/d3-tip/0.6.3/d3-tip.min.js",
